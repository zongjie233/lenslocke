{{template "header" .}}
<div class="px-6">

<h1 class="py-4 text-4xl semibold tracking-tight">FAQ page</h1>
<ul class="grid grid-cols-2 gap-16">
    {{range .}}
        {{template "qa" .}}
    {{end}}
</ul>
</div>
{{template "footer" .}}


{{define "qa"}}
<li class="border-t border-indigo-400 py-1 px-2">
<b>{{.Question}}</b> 
{{.Answer}}</li>
{{end}}

